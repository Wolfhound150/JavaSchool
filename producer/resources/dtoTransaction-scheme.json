{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "operType": {
      "type": "string",
      "enum": [
        "DEBIT",
        "CREDIT"
      ]
    },
    "amount": {
      "type": "number"
    },
    "number": {
      "type": "string"
    },
    "date": {
      "type": "number",
      "format": "date"
    }
  },
  "additionalProperties": false,
  "required": [
    "operType",
    "amount",
    "number",
    "date"
  ]
}